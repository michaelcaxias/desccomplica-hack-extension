{
    "manifest_version": 3,
    "name": "Sobrescrita Descomplica (V1 & V2)",
    "version": "2.0",
    "description": "Menu de alternância entre as táticas V1 (Network) e V2 (Design Patterns Vue)",
    "permissions": [
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "*://aulas.descomplica.com.br/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "v1/injected.js"
            ],
            "matches": [
                "*://aulas.descomplica.com.br/*"
            ]
        }
    ]
}